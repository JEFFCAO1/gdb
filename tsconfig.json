{
  "compilerOptions": {
    "allowJs": true,
    "noEmitOnError": false,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": ["dom", "es2015", "es2016.array.include"],
    "module": "commonjs",
    "moduleResolution": "node",
    "newLine": "LF",
    "outDir": "dist",
    "preserveConstEnums": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "downlevelIteration": true,
    "target": "es5"
  }
}